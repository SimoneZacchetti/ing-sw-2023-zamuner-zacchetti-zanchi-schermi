<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>

<!--
In StackPane, tutti gli elementi aggiunti vengono automaticamente centrati.
-->

<StackPane
        fx:id="mainPane"
        xmlns="http://javafx.com/javafx/8.0.171"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="it.polimi.ingsw.view.gui.StartWindowController">

        <Button
                fx:id="startButton"
                onAction="#onStartButtonClick"
                text="Start"
        />
</StackPane>
